    "@tanstack/react-query": "^5.24.7",
    "@types/google-map-react": "^2.1.9",
    "@types/styled-components": "^5.1.29",
    "babel-plugin-styled-components": "^2.1.4",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "firebaseui": "^6.1.0",
