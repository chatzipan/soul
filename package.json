{
  "name": "soul",
  "version": "1.0.0",
  "private": true,
  "description": "soul",
  "author": "Vasilis Chatzipanagiotis",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop",
    "emulators": "firebase use default && firebase emulators:start --import=./emulator-data --export-on-exit=./emulator-data",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "firebase deploy --only functions",
    "deploy-prod": "firebase use prod && firebase deploy --only functions",
    "typecheck": "tsc --noEmit",
    "upload-google-menu": "node ./scripts/upload-google-menu.js",
    "update-menu": "export GOOGLE_APPLICATION_CREDENTIALS=\"./google_cloud_keys.json\" && node ./scripts/update-menu.js"
  },
